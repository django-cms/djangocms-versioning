{
  "name": "djangoCMS",
  "private": true,
  "scripts": {
    "bundle": "gulp bundle",
    "bundle:coverage": "COVERAGE=true gulp bundle",
    "e2e": "npm run bundle:coverage && playwright test && nyc report --reporter=text-summary",
    "e2e:headed": "npm run bundle:coverage && playwright test --headed",
    "e2e:coverage:report": "nyc report --reporter=lcov --report-dir=coverage-e2e"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.5",
    "@playwright/test": "^1.49.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^10.0.0",
    "babel-plugin-istanbul": "^7.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-rewire": "^1.2.0",
    "browserslist-saucelabs": "^0.3.0",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.0",
    "eslint-plugin-compat": "^6.0.2",
    "exports-loader": "^5.0.0",
    "gulp": "^5.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-eslint-new": "^2.4.2",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^10.0.0",
    "gulp-sass": "^6.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-util": "^3.0.8",
    "imports-loader": "^5.0.0",
    "minimist": "^1.2.8",
    "nyc": "^17.1.0",
    "postcss-loader": "^8.2.0",
    "webpack": "^5.89.0"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/runtime": "^7.23.5",
    "escape-html": "^1.0.3",
    "lodash.debounce": "^4.0.8",
    "lodash.memoize": "^4.1.2",
    "nprogress": "^0.2.0",
    "raw-loader": "^4.0.2",
    "srcdoc-polyfill": "^1.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "coverage-e2e",
    "exclude": [
      "tests/**",
      "coverage/**",
      "coverage-e2e/**",
      "djangocms_versioning/static/djangocms_versioning/js/dist/**",
      "djangocms_versioning/static/djangocms_versioning/js/libs/**",
      "djangocms_versioning/static/djangocms_versioning/js/libs/api/**"
    ]
  }
}
